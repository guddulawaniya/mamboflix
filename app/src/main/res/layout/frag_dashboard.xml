<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/lytTop"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.HomeFragment"
        android:visibility="visible">

        <LinearLayout
            android:visibility="visible"
            android:id="@+id/lytTopAnimation"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/black"
            android:orientation="vertical">
            <!-- <include layout="@layout/shimmer_placeholder_home_layout" />-->
<!--

            <com.wenhui.shimmerimageview.ShimmerImageView
                android:id="@+id/shimmerImageView"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_450"
                android:layout_gravity="center"
                app:srcCompat="@drawable/image_loading"
                app:tint="@color/hintColor"
                app:startAnimation="true"
                app:maskSpecs_maskMode="src_in"
                android:visibility="gone"
                app:maskSpecs_dropOff="0.3"
                app:maskSpecs_maskColor="@color/hintColor"
                app:maskSpecs_animationDuration="2000"
                app:maskSpecs_intensity="0.2"
                app:maskSpecs_startDelayed="0"/>
-->

            <com.facebook.shimmer.ShimmerFrameLayout
                android:visibility="visible"
                android:id="@+id/shimmerFrameLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_350sdp"
                        android:layout_centerHorizontal="true"
                        app:civ_border_color="@color/hintColor"
                        android:background="@color/hintColor"
                        app:civ_border_width="@dimen/dp_2"
                        app:roundAsCircle="true" />

                    <!-- <View
                         android:layout_width="match_parent"
                         android:layout_height="@dimen/dp_450"
                         android:background="@color/hintColor"/>-->

                    <!-- <include layout="@layout/shimmer_placeholder_home_layout" />-->

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/black"
                        android:orientation="horizontal">



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_gravity="center_vertical"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:minWidth="@dimen/dp_100"
                                tools:text="@string/previews"
                                android:textSize="@dimen/sp_14"
                                android:background="@color/hintColor"
                                android:textColor="@color/hintColor"/>


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/dp_4"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">



                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:layout_width="85dp"
                                    android:layout_height="85dp"
                                    android:layout_centerHorizontal="true"
                                    app:civ_border_color="@color/hintColor"
                                    android:background="@color/hintColor"
                                    app:civ_border_width="@dimen/dp_2"
                                    android:layout_marginLeft="@dimen/dp_8"
                                    app:roundAsCircle="true" />

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:layout_width="85dp"
                                    android:layout_height="85dp"
                                    android:layout_centerHorizontal="true"
                                    app:civ_border_color="@color/hintColor"
                                    android:background="@color/hintColor"
                                    app:civ_border_width="@dimen/dp_2"
                                    android:layout_marginLeft="@dimen/dp_8"
                                    app:roundAsCircle="true" />

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:layout_width="85dp"
                                    android:layout_height="85dp"
                                    android:layout_centerHorizontal="true"
                                    app:civ_border_color="@color/hintColor"
                                    android:background="@color/hintColor"
                                    app:civ_border_width="@dimen/dp_2"
                                    android:layout_marginLeft="@dimen/dp_8"
                                    app:roundAsCircle="true" />


                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>


                    <!-- new layout-->
                    <LinearLayout
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:text="@string/previews"
                            android:background="@color/hintColor"
                            android:textSize="@dimen/sp_16"
                            android:textColor="@color/hintColor"/>


                        <LinearLayout android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:padding="@dimen/dp_4"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">



                            <RelativeLayout android:layout_width="110dp"
                                android:layout_height="160dp"
                                android:padding="@dimen/dp_2"
                                android:gravity="center_vertical"
                                android:layout_margin="2dp"
                                android:orientation="vertical">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    app:cardCornerRadius="@dimen/dp_5"
                                    app:cardUseCompatPadding="false">

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">



                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_centerHorizontal="true"
                                            android:scaleType="fitXY"
                                            android:background="@color/hintColor" />

                                    </RelativeLayout>
                                </androidx.cardview.widget.CardView>

                            </RelativeLayout>

                            <RelativeLayout android:layout_width="110dp"
                                android:layout_height="160dp"
                                android:padding="@dimen/dp_2"
                                android:gravity="center_vertical"
                                android:layout_margin="2dp"
                                android:orientation="vertical">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    app:cardCornerRadius="@dimen/dp_5"
                                    app:cardUseCompatPadding="false">

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">



                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_centerHorizontal="true"
                                            android:scaleType="fitXY"
                                            android:background="@color/hintColor" />

                                    </RelativeLayout>
                                </androidx.cardview.widget.CardView>

                            </RelativeLayout>

                            <RelativeLayout android:layout_width="110dp"
                                android:layout_height="160dp"
                                android:padding="@dimen/dp_2"
                                android:gravity="center_vertical"
                                android:layout_margin="2dp"
                                android:orientation="vertical">
                                <androidx.cardview.widget.CardView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    app:cardCornerRadius="@dimen/dp_5"
                                    app:cardUseCompatPadding="false">

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">



                                        <ImageView

                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_centerHorizontal="true"
                                            android:scaleType="fitXY"
                                            android:background="@color/hintColor" />

                                    </RelativeLayout>
                                </androidx.cardview.widget.CardView>

                            </RelativeLayout>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>

            </com.facebook.shimmer.ShimmerFrameLayout>

        </LinearLayout>



        <com.google.android.material.appbar.AppBarLayout
            android:visibility="visible"
            android:id="@+id/room_details_app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:fitsSystemWindows="true"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

            <com.google.android.material.appbar.CollapsingToolbarLayout
                android:id="@+id/room_details_collapsing_toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:contentInsetEnd="0dp"
                android:contentInsetRight="0dp"
                android:fitsSystemWindows="true"
                android:paddingTop="0dp"
                android:paddingRight="0dp"
                app:contentInsetEnd="0dp"
                app:contentInsetRight="0dp"
                app:contentScrim="#000"
                app:layout_scrollFlags="scroll|exitUntilCollapsed|snap"
                app:scrimAnimationDuration="300">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fitsSystemWindows="true"
                    app:layout_collapseMode="parallax">

                    <ImageView
                        android:id="@+id/ivBanner"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_400sdp"
                        android:scaleType="fitXY"
                        android:src="@drawable/mamboflix_banner"
                        android:visibility="visible"/>

<!--                    <com.google.android.exoplayer2.ui.SimpleExoPlayerView-->
<!--                        android:layout_width="match_parent"-->
<!--                        android:layout_height="@dimen/dp_450"-->
<!--                        android:id="@+id/exo_player"-->
<!--                        app:use_controller="false"/>-->
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_400sdp"
                        android:src="@drawable/mamboflix_banner"
                        android:scaleType="fitXY"/>

                    <RelativeLayout
                        android:layout_centerHorizontal="true"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:visibility="visible"
                            android:id="@+id/listLyt"
                            android:layout_alignParentBottom="true"
                            android:layout_centerHorizontal="true"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:paddingTop="@dimen/dp_8"
                                android:paddingRight="@dimen/dp_10"
                                android:paddingLeft="@dimen/dp_1"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tvMovie"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:fontFamily="@font/din_medium"
                                    android:textColor="@color/white"
                                    android:textSize="11sp" />

                                <View
                                    android:layout_width="3dp"
                                    android:layout_height="3dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="6dp"
                                    android:layout_marginRight="6dp"
                                    android:background="@drawable/circle_white" />

                                <TextView
                                    android:id="@+id/tvYear"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:fontFamily="@font/din_medium"
                                    android:textColor="@color/white"
                                    android:textSize="11sp" />

                                <View
                                    android:layout_width="3dp"
                                    android:layout_height="3dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="6dp"
                                    android:layout_marginRight="6dp"
                                    android:background="@drawable/circle_white" />

                                <TextView
                                    android:id="@+id/tvTime"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:fontFamily="@font/din_medium"
                                    android:textColor="@color/white"
                                    android:textSize="11sp" />

                                <View
                                    android:layout_width="3dp"
                                    android:layout_height="3dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="6dp"
                                    android:layout_marginRight="6dp"
                                    android:background="@drawable/circle_white" />

                                <TextView
                                    android:id="@+id/tvCategory"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:fontFamily="@font/din_medium"
                                    android:textColor="@color/white"
                                    android:textSize="11sp" />

                                <View
                                    android:visibility="gone"
                                    android:layout_width="3dp"
                                    android:layout_height="3dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="6dp"
                                    android:layout_marginRight="6dp"
                                    android:background="@drawable/circle_white" />

                                <TextView
                                    android:visibility="gone"
                                    android:id="@+id/tvCertificat"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:fontFamily="@font/din_medium"
                                    android:textColor="@color/white"
                                    android:textSize="11sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:padding="@dimen/dp_10"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:weightSum="3"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/tvAddList"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:gravity="center"
                                    android:layout_weight="1">
                                    <ImageView
                                        android:id="@+id/ivMyList"
                                        android:layout_width="@dimen/dp_18"
                                        android:layout_height="@dimen/dp_18"
                                        android:src="@drawable/my_list"/>

                                    <TextView
                                        android:layout_gravity="center_horizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="@dimen/dp_5"
                                        android:text="@string/my_list"
                                        android:textSize="@dimen/sp_14"
                                        android:fontFamily="@font/din_medium"
                                        android:textColor="@color/white"/>

                                </LinearLayout>

                                <View
                                    android:layout_width="@dimen/dp_40"
                                    android:layout_height="match_parent"
                                    android:background="@color/transparent"/>

                                <LinearLayout
                                    android:id="@+id/btnPlay"
                                    android:layout_width="@dimen/_80sdp"
                                    android:layout_height="wrap_content"
                                    android:paddingRight="@dimen/dp_10"
                                    android:paddingLeft="@dimen/dp_10"
                                    android:minWidth="95dp"
                                    android:paddingTop="@dimen/dp_5"
                                    android:paddingBottom="@dimen/dp_5"
                                    android:background="@drawable/suare_corner_white"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_weight="1">

                                    <ImageView
                                        android:layout_width="@dimen/sp_14"
                                        android:layout_height="@dimen/sp_14"
                                        android:src="@drawable/play_black"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/dp_8"
                                        android:text="@string/play"
                                        android:fontFamily="@font/din_medium"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/sp_14"
                                        android:textStyle="bold" />

                                </LinearLayout>


                                <View
                                    android:layout_width="@dimen/dp_40"
                                    android:layout_height="match_parent"
                                    android:background="@color/transparent"/>

                                <LinearLayout
                                    android:id="@+id/lytInfo"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:gravity="center"
                                    android:layout_weight="1">
                                    <ImageView
                                        android:layout_width="@dimen/dp_18"
                                        android:layout_height="@dimen/dp_18"
                                        android:src="@drawable/info"/>

                                    <TextView
                                        android:layout_marginTop="@dimen/dp_5"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/info"
                                        android:fontFamily="@font/din_medium"
                                        android:textSize="@dimen/sp_14"
                                        android:textColor="@color/white"/>
                                </LinearLayout>
                            </LinearLayout>


                        </LinearLayout>
                    </RelativeLayout>

                </RelativeLayout>


                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_8"
                    app:contentInsetStart="0dp"
                    app:layout_collapseMode="pin">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/ivLogo"
                            android:layout_width="@dimen/dp_24"
                            android:layout_height="@dimen/dp_24"
                            android:src="@drawable/logo_small" />


                        <ImageView
                            android:layout_width="24dp"
                            android:layout_height="24dp"
                            android:layout_alignParentRight="true"
                            android:scaleType="fitEnd"
                            android:id="@+id/ivSearch"
                            android:src="@drawable/search" />
                    </RelativeLayout>


                </androidx.appcompat.widget.Toolbar>

            </com.google.android.material.appbar.CollapsingToolbarLayout>

        </com.google.android.material.appbar.AppBarLayout>



        <androidx.core.widget.NestedScrollView
            android:id="@+id/nsvMain"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#000000"
            android:fillViewport="true"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <LinearLayout
                android:id="@+id/tv_lication"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="@dimen/dp_10"
                    android:orientation="vertical">

                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytPreview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_preview"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/previews"
                            android:fontFamily="@font/din_medium"
                            android:textSize="@dimen/sp_14"
                            android:textColor="@color/white"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvPreviews"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            tools:listitem="@layout/row_previews_list"
                            android:orientation="horizontal" />

                    </LinearLayout>

                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytContinue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/continue_watching"
                            android:fontFamily="@font/din_medium"
                            android:textSize="@dimen/sp_16"
                            android:textColor="@color/white"/>



                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvContinue"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            android:nestedScrollingEnabled="false"/>


                    </LinearLayout>


                    <!--My list-->

                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytMyList"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/my_list"
                            android:fontFamily="@font/din_medium"
                            android:textSize="@dimen/sp_16"
                            android:textColor="@color/white"/>



                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvMyList"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            android:nestedScrollingEnabled="false"/>


                    </LinearLayout>

                    <!--Because of you-->
                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytOfYou"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/because_of_you_watched_star_wars"
                            android:textSize="@dimen/sp_16"
                            android:fontFamily="@font/din_medium"
                            android:textColor="@color/white"/>



                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvOfYou"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            android:nestedScrollingEnabled="false"/>


                    </LinearLayout>

                    <!--Trending Now-->
                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytTrendingNow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/trending_now"
                            android:fontFamily="@font/din_medium"
                            android:textSize="@dimen/sp_16"
                            android:textColor="@color/white"/>



                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvTrendingNow"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            android:nestedScrollingEnabled="false"/>


                    </LinearLayout>


                    <!--Most Watched-->
                    <LinearLayout
                        android:visibility="invisible"
                        android:id="@+id/lytMostWatched"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:orientation="vertical">


                        <TextView
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/most_watched"
                            android:fontFamily="@font/din_medium"
                            android:textSize="@dimen/sp_16"
                            android:textColor="@color/white"/>



                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rvMostWatched"
                            android:layout_marginTop="@dimen/dp_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clipToPadding="false"
                            android:nestedScrollingEnabled="false"/>


                    </LinearLayout>

                    <!-- Category-->
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rvAll"
                        android:layout_marginTop="@dimen/dp_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clipToPadding="false"
                        android:nestedScrollingEnabled="false"/>



                </LinearLayout>
            </LinearLayout>


        </androidx.core.widget.NestedScrollView>



    </androidx.coordinatorlayout.widget.CoordinatorLayout>

    <!-- </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>-->
</layout>